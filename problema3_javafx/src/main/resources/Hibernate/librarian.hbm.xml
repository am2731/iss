<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="model.Librarian"
           table="Librarians">
        <id name="id" column="id">
            <generator class="increment"/>
        </id>
        <property
                name="firstName"
                column="firstname"/>
        <property
                name="lastName"
                column="lastname"/>
        <property
                name="password"
                column="password"/>
        <property
                name="email"
                column="email"/>
    </class>
</hibernate-mapping>